{
  "env": {
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "cjs"
          }
        ],
        [
          "@babel/preset-typescript",
          {
            "allowDeclareFields": true
          }
        ]
      ]
    },
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        [
          "@babel/preset-typescript",
          {
            "allowDeclareFields": true
          }
        ]
      ]
    }
  }
}
